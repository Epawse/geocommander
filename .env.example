# GeoCommander Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Tianditu Map Token (required for basemaps)
# Get your token at: https://console.tianditu.gov.cn/
VITE_TIANDITU_TOKEN=your_tianditu_token_here

# Cesium Ion Token (optional, for terrain)
# Get your token at: https://cesium.com/ion/
VITE_CESIUM_ION_TOKEN=your_cesium_ion_token_here

# Backend WebSocket URL
VITE_WS_URL=ws://localhost:8765/ws

# Backend API URL
VITE_API_URL=http://localhost:8765
